# Vault contract address (from deploy output). Leave empty to use default.
VITE_VAULT_ADDRESS=0xBa9Fa63E2F9E776f7F08371FeC95dD1FCd34E137

# pUSD token address (only when using deploy:pusd:testnet). Frontend also reads from vault.stablecoin().
VITE_PUSD_ADDRESS=0x11a8cAAD58BedBCbe0471d0dDf64516624d66279

# Vault type: "full" = AegisVault (repay, withdraw); "demo" = AegisVaultDemo (no repay/withdraw)
VITE_VAULT_TYPE=full

